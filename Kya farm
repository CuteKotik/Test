local function pressKey(topress)
    fire(player.Character.Remotes.KeyEvent, key, topress, "Down", player:GetMouse().Hit, nil, workspace.Camera.CFrame)
end

while true do
    if array.autofarm then
        if player.PlayerFolder.CanAct.Value then
            pressKey("Mouse1")
        end
    end
    wait()
end
